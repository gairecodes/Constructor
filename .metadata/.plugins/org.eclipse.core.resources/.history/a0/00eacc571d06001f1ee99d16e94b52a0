package Looping;
import java.util.Scanner;

public class PrimeorNot {
  public static void main(String[] args) {
	 Scanner sc = new Scanner(System.in);
	 
	 int num;
	 
	 System.out.println("enter the number you want to check:");
	 num = sc.nextInt();
	 
	 for(int i = 2 ; i<num; i++) {
		 
		 int flag;
		 if (num % i== 0 ) {
		return flag=0;
		 }
	 }
	 
	 
	 
	 
}
}
